{
  "name": "logans3d-v4",
  "version": "1.0.0",
  "description": "Logan's 3D Creations - Modern e-commerce platform for 3D printing business",
  "main": "tailwind.config.js",
  "scripts": {
    "build:css": "npm run build:css:public && npm run build:css:admin",
    "build:css:public": "postcss public/css/public-input.css -o public/css/public-styles.css",
    "build:css:admin": "postcss public/css/admin-input.css -o public/css/admin-styles.css",
    "watch:css": "postcss public/css/input.css -o public/css/styles.css --watch"
  },
  "keywords": [
    "3d-printing",
    "ecommerce",
    "go",
    "templ",
    "tailwind"
  ],
  "author": "Logan Lanou",
  "license": "ISC",
  "devDependencies": {
    "@tailwindcss/cli": "^4.0.0",
    "@tailwindcss/postcss": "^4.1.12",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^4.1.12"
  }
}
