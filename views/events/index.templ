								<div class="absolute top-4 right-4 w-16 h-16 bg-gradient-to-br from-emerald-400 to-blue-500 rounded-full blur-xl animate-pulse"></div>
								<div class="absolute bottom-4 left-4 w-12 h-12 bg-gradient-to-br from-teal-400 to-emerald-500 rounded-full blur-lg animate-pulse animation-delay-2000"></div>
							</div>
							<div class="relative z-10">
								<h2 class="text-5xl font-bold text-white mb-12 group-hover:scale-105 transition-transform duration-300">
									<span class="bg-gradient-to-r from-blue-300 to-emerald-400 bg-clip-text text-transparent">Stay Updated</span>
								</h2>
								<p class="text-xl text-slate-300 mb-16 leading-relaxed">
									Get notified about upcoming events, new workshops, and special offers.
								</p>
								<form class="max-w-lg mx-auto flex flex-col sm:flex-row gap-6">
									<input
										type="email"
										placeholder="Enter your email"
										class="flex-1 px-6 py-4 bg-slate-700/50 border border-slate-600/50 rounded-xl text-white placeholder-slate-400 focus:outline-none focus:ring-2 focus:ring-emerald-500/50 focus:border-emerald-500/50 backdrop-blur-sm transition-all duration-300"
									/>
									<button
										type="submit"
										class="bg-gradient-to-r from-emerald-600 to-teal-600 text-white px-10 py-4 rounded-xl font-semibold hover:from-emerald-700 hover:to-teal-700 transition-all duration-300 shadow-lg hover:shadow-xl hover:shadow-emerald-500/25 transform hover:-translate-y-1"
									>
										Subscribe
									</button>
								</form>
							</div>
						</div>
					</div>
				</section>
			</div>
		</div>
	}
}
