package innovation

import (
	"github.com/loganlanou/logans3d-v4/internal/auth"
	"github.com/loganlanou/logans3d-v4/views/layout"
)

templ Manufacturing(authCtx *auth.Context) {
	@layout.Base(layout.Meta{
		Title:       "Advanced Manufacturing - Logan's 3D Creations",
		Description: "Explore our advanced manufacturing capabilities including multi-material printing, precision engineering, and state-of-the-art 3D printing technology.",
		Keywords:    "advanced manufacturing, 3D printing technology, multi-material printing, precision engineering, digital fabrication",
	}, authCtx) {
		<div class="min-h-screen bg-gradient-to-br from-slate-900 via-slate-800 to-slate-900 relative overflow-hidden">
			<!-- Background Elements -->
			<div class="absolute inset-0 opacity-15">
				<div class="absolute top-1/4 left-1/4 w-72 h-72 bg-gradient-to-r from-red-500/20 to-slate-400/20 rounded-full mix-blend-multiply filter blur-2xl animate-pulse"></div>
				<div class="absolute top-3/4 right-1/4 w-72 h-72 bg-gradient-to-r from-slate-300/20 to-red-600/20 rounded-full mix-blend-multiply filter blur-2xl animate-pulse animation-delay-2000"></div>
			</div>
			<!-- Grid Pattern -->
			<div class="absolute inset-0 opacity-10">
				<div class="h-full w-full" style="background-image: linear-gradient(rgba(148, 163, 184, 0.2) 1px, transparent 1px), linear-gradient(90deg, rgba(148, 163, 184, 0.2) 1px, transparent 1px); background-size: 40px 40px;"></div>
			</div>
			<div class="relative z-10">
				@ManufacturingHero()
				@ManufacturingCapabilities()
				@TechnologyStack()
				@ProcessExcellence()
			</div>
		</div>
	}
}

templ ManufacturingHero() {
	<section class="pt-32 pb-24 px-8 sm:px-12 lg:px-16">
		<div class="max-w-6xl mx-auto">
			<div class="text-center">
				<a href="/innovation" class="inline-flex items-center px-6 py-3 bg-slate-800/50 border border-slate-600/50 rounded-full text-slate-300 text-sm hover:bg-slate-700/50 hover:border-slate-500/50 transition-all duration-300 mb-12">
					<svg class="w-4 h-4 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
						<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 19l-7-7m0 0l7-7m-7 7h18"></path>
					</svg>
					Back to Innovation & Quality
				</a>
				<h1 class="text-6xl sm:text-7xl lg:text-8xl font-black leading-[0.9] mb-12 hover:scale-105 transition-transform duration-500 cursor-default tracking-tight">
					<div class="inline-block">
						<span class="bg-gradient-to-r from-red-400 via-slate-300 to-red-500 bg-clip-text text-transparent hover:from-red-300 hover:via-slate-200 hover:to-red-400 transition-all duration-300">
							Advanced
						</span>
					</div>
					<br/>
					<div class="inline-block relative">
						<span class="bg-gradient-to-r from-slate-300 via-red-300 to-slate-400 bg-clip-text text-transparent hover:from-slate-200 hover:via-red-200 hover:to-slate-300 transition-all duration-300">
							Manufacturing
						</span>
						<div class="absolute -bottom-4 left-0 w-full h-2 bg-gradient-to-r from-red-500 via-slate-400 to-red-600 rounded-full opacity-60 shadow-lg shadow-red-500/30"></div>
					</div>
				</h1>
				<p class="text-xl sm:text-2xl text-slate-300 mb-16 max-w-4xl mx-auto leading-relaxed">
					State-of-the-art digital fabrication combining cutting-edge technology, precision engineering, and innovative manufacturing processes.
				</p>
			</div>
		</div>
	</section>
}

templ ManufacturingCapabilities() {
	<section class="px-8 sm:px-12 lg:px-16 py-24 border-t border-slate-700/50">
		<div class="max-w-6xl mx-auto">
			<div class="text-center mb-20">
